{
    "manifest_version": 3,
    "name": "Chess.com Cheats",
    "version": "0.1.1",
    "icons": {
        "128": "icon128.png"
    },
    "action": {},
    "content_scripts": [
        {
            "matches": ["*://*.chess.com/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "permissions": ["activeTab"]
}
